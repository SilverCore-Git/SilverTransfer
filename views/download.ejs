        <script type="module" defer>


            const size = "<%= fileSize %>";
            const name = "<%= fileName %>";


            history.pushState(null, "", `?page=main&fname=${name}&fsize=${size}&file=1`);
            

            function salert(text, type) {
                history.pushState(null, "", `?page=main&fname=${name}&fsize=${size}&file=1&salert=1&text=${text}&type=${type}`);
                Swal.fire({
                    title: 'SilverTransfer',
                    html: text,
                    icon: type,
                    confirmButtonText: 'Ok !',
                    background: '#f0f0f0',
                    color: '#333',
                    confirmButtonColor: '#ff4090',
                    timer: (789 * 789)*789,
                    showClass: {
                        popup: 'animate__animated animate__fadeIn'
                    },
                    hideClass: {
                        popup: 'animate__animated animate__fadeOut'
                    }
                });
            }


            document.getElementById('loader').style.display = 'none';
            document.getElementById('download').style.display = 'none';


            document.addEventListener("DOMContentLoaded", () => {

                const continu_btn = document.getElementById('btn_launch_decrypt');

                continu_btn.addEventListener('click', async () => {

                    history.pushState(null, "", `?page=loader&fname=${name}&fsize=${size}&file=1`);

                    setTimeout(() => {
                        document.getElementById('main').style.display = 'none';
                        document.getElementById('loader').style.display = 'flex';
                    }, 300);

                    history.pushState(null, "", `?page=loader&fname=${name}&fsize=${size}&file=1&statu=fetching`);

                    const passwd = "<%= passwd %>";
                    await fetch(`/data/<%= fileID %>?action=decrypt&passwd=${passwd}`)
                        .then(response => response.json())
                        .then(res => {

                            history.pushState(null, "", `?page=loader&fname=${name}&fsize=${size}&file=1&statu=res`);

                            if (res.error) {
                                salert(res.message.silver, 'error');
                                document.getElementById('main').style.display = 'flex'
                                document.getElementById('loader').style.display = 'none'
                                return history.pushState(null, "", `?page=main&fname=${name}&fsize=${size}&file=1&statu=error`);
                            };

                        })
                        .catch(err => {
                            console.error('Une errreur est survenue : ', err || err.message);
                            salert('Une errreur est survenue !', 'error');
                            document.getElementById('main').style.display = 'flex'
                            document.getElementById('loader').style.display = 'none'
                            return history.pushState(null, "", `?page=main&fname=${name}&fsize=${size}&file=1&statu=error&msg=${err}`);
                        });

                    async function ending() {
                        history.pushState(null, "", `?page=download&fname=${name}&fsize=${size}&file=1&statu=success`);
                        document.getElementById('loader').style.display = 'none'
                        document.getElementById('download').style.display = 'flex';
                    };

                    document.getElementById('download_btn').addEventListener('click', () => {
                        window.open(`/data/<%= fileID %>?action=download`, '_blank');
                    })

                    const intervalID = setInterval(async () => {

                        await fetch('/data/status?id=<%= fileID %>')
                            .then(response => response.json())
                            .then(res => {

                                if (res.error) {
                                    document.getElementById('loader').style.display = 'none'
                                    document.getElementById('main').style.display = 'flex';
                                    return salert(res.message.silver, 'error');
                                }

                                if (res.end) {
                                    clearInterval(intervalID);
                                    return ending();
                                }

                            });

                    }, 1000);


                });

            });

        </script>


