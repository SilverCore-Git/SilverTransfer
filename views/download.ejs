                <script defer>

                    const loader1 = document.querySelector('.loader-1');
                    const loader2 = document.querySelector('.loader-2');

                    loader1.style.display = 'none';
                    loader2.style.display = 'flex';

                    setInterval(() => {
                        const isLoader1Visible = loader1.style.display === 'none';

                        loader1.style.display = isLoader1Visible ? 'flex' : 'none';
                        loader2.style.display = isLoader1Visible ? 'none' : 'flex';
                    }, 10000);

                    const statustext = document.getElementById('statu_text');

                    const frames = [
                        "Déchiffrement en cours",
                        "Déchiffrement en cours.",
                        "Déchiffrement en cours..",
                        "Déchiffrement en cours..."
                    ];

                    let tailledetesmorts = 0;

                    setInterval(() => {
                        
                        statustext.innerText = frames[tailledetesmorts]
                        tailledetesmorts++

                        if (tailledetesmorts === 4) { tailledetesmorts = 0 }

                    }, 500);

                </script>
            
