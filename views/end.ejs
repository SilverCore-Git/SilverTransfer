font-size: 14px;
white-space: nowrap;
opacity: 1;
visibility: visible;
transition: opacity 0.2s ease-in-out;
}

.social-icon:hover::before {
content: "";
position: absolute;
bottom: 100%;
left: 50%;
transform: translateX(-50%);
border: 6px solid transparent;
border-top-color: black;
margin-bottom: -12px;
opacity: 1;
visibility: visible;
transition: opacity 0.2s ease-in-out;
}

.social-icon::after,
.social-icon::before {
opacity: 0;
visibility: hidden;
transition: opacity 0.2s ease-in-out;
}
